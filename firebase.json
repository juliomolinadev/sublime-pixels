{
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"hosting": {
		"site": "sublimepixels",
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
	},
	"storage": {
		"rules": "storage.rules"
	},
	"emulators": {
		"auth": {
			"port": 3001
		},
		"firestore": {
			"port": 8080
		},
		"hosting": {
			"port": 5000
		},
		"storage": {
			"port": 9199
		},
		"ui": {
			"enabled": true
		},
		"singleProjectMode": true
	}
}
